<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.roomservices.RoomServiceFragment">

    <FrameLayout
        android:id="@+id/frameLayoutRentCheckout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/botNavRentCheckout"
        android:layout_alignTop="@+id/botNavRentCheckout"
        android:layout_alignParentTop="true">

    </FrameLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/botNavRentCheckout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:menu="@menu/rent_checkout_bottom_navigation"
        tools:layout_editor_absoluteX="0dp" />
</RelativeLayout>